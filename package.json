{
  "name": "greenhacker-portfolio",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "build:netlify": "npm run db:generate:safe && npm run build:check-deps && SKIP_TYPE_CHECK=true next build",
    "build:check-deps": "npm list prisma tailwindcss || npm install prisma tailwindcss autoprefixer postcss --legacy-peer-deps",
    "build:skip-types": "prisma generate && SKIP_TYPE_CHECK=true next build",
    "build:simple": "npm install prisma tailwindcss autoprefixer postcss --legacy-peer-deps && npx prisma generate --no-engine && SKIP_TYPE_CHECK=true npx next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:generate:netlify": "prisma generate --no-engine",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "postinstall": "npm run db:generate:safe",
    "db:generate:safe": "prisma generate --no-engine || prisma generate || echo 'Prisma generation failed, continuing...'"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
